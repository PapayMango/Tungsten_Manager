<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--<AnchorPane xmlns="http://javafx.com/javafx"-->
<!--            xmlns:fx="http://javafx.com/fxml"-->
<!--            prefHeight="400.0" prefWidth="600.0" fx:id="main" stylesheets="@evaluation.css">-->
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="evaluation.Controller"
            prefHeight="400.0" prefWidth="600.0" fx:id="main" stylesheets="@evaluation.css">
    <AnchorPane fx:id="details" AnchorPane.topAnchor="20" AnchorPane.leftAnchor="30">
        <VBox>
            <HBox fx:id="title"><Label text="Lot  No."/><Label fx:id="lot"/></HBox>
            <HBox><Label text="生産日 : "/><Label fx:id="date"/><Label text="生産量 : "/><Label fx:id="product"/><Label text="PH : "/><Label fx:id="ph"/></HBox>
            <HBox><Label text="固形分濃度 : "/><Label fx:id="concentration"/><Label text="総出荷量 : "/><Label fx:id="shipment"/><Label text="保管場所 : "/><Label fx:id="location_"/></HBox>
<!--            <HBox><Label text="固形分濃度 : "/><Label fx:id="concentration"/><Label text="総出荷量 : "/><Label fx:id="shipment"/><Label text="保管場所 : "/></HBox>-->
        </VBox>
    </AnchorPane>
    <AnchorPane fx:id="filter" AnchorPane.topAnchor="145" AnchorPane.leftAnchor="30">
        <VBox>
            <HBox><Label text="濃度 : "/><TextField fx:id="from_Concentration" maxWidth="50" /><Label>～</Label><TextField fx:id="to_Concentration" maxWidth="50"/><Label text="添加剤 : "/><TextField fx:id="additive" maxWidth="100" /></HBox>
            <HBox><Label text="PH : "/><TextField fx:id="from_Ph" maxWidth="50" /><Label>～</Label><TextField fx:id="to_Ph" maxWidth="50"/><Label text="バインダー : "/><TextField fx:id="binder" maxWidth="100" /></HBox>
            <HBox><Label text="消臭 : "/><TextField fx:id="deodorize" maxWidth="65" /><Label text="メチレン : "/><TextField fx:id="methylene" maxWidth="65" /><Label text="ゴキブリ : "/><TextField fx:id="cockroach" maxWidth="65" /></HBox>
        </VBox>
    </AnchorPane>
    <Label text="出荷データ一覧" AnchorPane.topAnchor="10" AnchorPane.rightAnchor="10" onMouseClicked="#changePage"/>
    <TableView fx:id="result_tb" prefWidth="940" prefHeight="490" AnchorPane.topAnchor="280" AnchorPane.leftAnchor="30">
        <columns>
            <TableColumn fx:id="column_concentration" prefWidth="80" text="濃度"/>
            <TableColumn fx:id="column_ph" prefWidth="80" text="PH"/>
            <TableColumn fx:id="column_additive" prefWidth="150" text="添加剤"/>
            <TableColumn fx:id="column_binder" prefWidth="150" text="バインダー"/>
            <TableColumn fx:id="column_deodorize" prefWidth="80" text="消臭"/>
            <TableColumn fx:id="column_methylene" prefWidth="80" text="メチレン"/>
            <TableColumn fx:id="column_cockroach" prefWidth="80" text="ゴキブリ"/>
            <TableColumn fx:id="column_shipment" prefWidth="80" text="出荷量"/>
            <TableColumn fx:id="column_date" prefWidth="160" text="最終更新日"/>
        </columns>
    </TableView>
</AnchorPane>
