<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="evaluation.Controller"
            prefHeight="400.0" prefWidth="600.0" fx:id="main" stylesheets="@evaluation.css">
    <AnchorPane fx:id="details" AnchorPane.topAnchor="30" AnchorPane.leftAnchor="30">
        <VBox>
            <HBox fx:id="title"><Label text="Lot  No."/><Label fx:id="lot"/></HBox>
            <HBox><Label text="生産日 : "/><Label fx:id="date"/><Label text="生産量 : "/><Label fx:id="product"/><Label text="PH : "/><Label fx:id="ph"/></HBox>
            <HBox><Label text="固形分濃度 : "/><Label fx:id="concentration"/><Label text="総出荷量 : "/><Label fx:id="shipment"/><Label text="保管場所 : "/><Label fx:id="location"/></HBox>
        </VBox>
    </AnchorPane>
    <AnchorPane fx:id="filter" AnchorPane.topAnchor="120" AnchorPane.leftAnchor="30">
        <VBox>
            <HBox><Label text="濃度 : "/></HBox>
            <HBox><Label text="PH : "/></HBox>
            <HBox><Label text="消臭 : "/></HBox>
        </VBox>
    </AnchorPane>
    <TableView fx:id="result_tb" prefWidth="940" prefHeight="525" AnchorPane.topAnchor="250" AnchorPane.leftAnchor="30">
        <columns>
            <TableColumn fx:id="column_concentration" prefWidth="170" text="濃度"/>
            <TableColumn fx:id="column_ph" prefWidth="90" text="PH"/>
            <TableColumn fx:id="column_additive" prefWidth="90" text="添加剤"/>
            <TableColumn fx:id="column_binder" prefWidth="150" text="バインダー"/>
            <TableColumn fx:id="column_deodorize" prefWidth="75" text="消臭"/>
            <TableColumn fx:id="column_methylene" prefWidth="225" text="メチレン"/>
            <TableColumn fx:id="column_cockroach" prefWidth="125" text="ゴキブリ"/>
            <TableColumn fx:id="column_shipment" prefWidth="125" text="出荷量"/>
        </columns>
    </TableView>
</AnchorPane>
